                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#aaffffff"
                                android:text="Time"
                                android:textSize="14sp"
                                android:layout_gravity="start"
                                android:textStyle="bold" />
                            <TextView
