                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:text="Normal Viginal"
                            android:layout_gravity="start"
                            android:textColor="#333333"
                            android:textStyle="bold" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </android.support.v7.widget.CardView>


