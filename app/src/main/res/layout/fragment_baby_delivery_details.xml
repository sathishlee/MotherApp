                            android:textAlignment="textStart"
                            android:textColor="#fff"
                            android:textStyle="bold" />
                    </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
