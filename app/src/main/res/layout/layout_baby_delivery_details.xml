<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/delivery_details">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="6dp"
        android:orientation="vertical"
        android:padding="16.0dip"
        app:cardBackgroundColor="@android:color/white"
        app:cardElevation="2.0dip">
        <!--welcome-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="7dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false"
                android:weightSum="2"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/delivery_details"
                        android:textAlignment="textStart"
                        android:textColor="#2F80ED"
                        android:textSize="@dimen/text_label"
                        android:textStyle="bold" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:paddingTop="4.0dip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View
                    android:layout_gravity="center"
                    android:background="@drawable/header_line"
                    android:layout_width="match_parent"
                    android:layout_height="5.0dp"
                    android:layout_weight="1.0"
                    android:layerType="software" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false"
                android:weightSum="2"
                android:layout_margin="2dp"
                android:orientation="horizontal">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/background_image"
                    android:layout_margin="4dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/date"
                        app:srcCompat="@drawable/calendar"
                        tools:ignore="VectorDrawableCompat" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/date"
                            android:textSize="@dimen/text_primary"
                            android:textAlignment="textStart"
                            android:textColor="#aaffffff"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/txt_delivery_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="24-02-2018"
                            android:textAlignment="textStart"
                            android:textColor="#fff"
                            android:textSize="@dimen/text_secondary" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/background_image"
                    android:layout_margin="4dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:layout_gravity="center"
                        android:contentDescription="@string/time"
                        app:srcCompat="@drawable/clock"
                        tools:ignore="VectorDrawableCompat" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="4dp"
                        android:orientation="vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="#aaffffff"
                            android:text="@string/time"
                            android:textSize="@dimen/text_primary"
                            android:layout_gravity="start"
                            android:textStyle="bold" />
                        <TextView
                            android:id="@+id/txt_delivery_time"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="02:30 PM"
                            android:layout_gravity="start"
                            android:textColor="#fff"
                            android:textSize="@dimen/text_secondary"/>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:baselineAligned="false"
                android:weightSum="2"
                android:layout_margin="2dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="1dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/place"
                        android:textAlignment="textStart"
                        android:textSize="@dimen/text_primary"
                        android:textColor="@color/secondary"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/txt_delivery_place"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="HSC"
                        android:textSize="@dimen/text_secondary"
                        android:textAlignment="textStart"
                        android:textColor="@color/text_primary"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="1dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:text="@string/type"
                        android:textSize="@dimen/text_primary"
                        android:layout_gravity="start"
                        android:textColor="@color/secondary"
                        android:textStyle="bold" />
                    <TextView
                        android:id="@+id/txt_delivery_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:text="Normal Viginal"
                        android:layout_gravity="start"
                        android:textSize="@dimen/text_secondary"
                        android:textColor="@color/text_primary"/>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </android.support.v7.widget.CardView>


